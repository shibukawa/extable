{
  "name": "@extable/core",
  "version": "0.3.9",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./ssr": {
      "import": "./dist/ssr/index.js",
      "types": "./dist/ssr/index.d.ts"
    },
    "./style.css": "./style.css",
    "./index.css": "./dist/core.css"
  },
  "files": [
    "dist",
    "style.css",
    "src/styles.css",
    "src/styles-overlays.css",
    "!dist/**/*.map",
    "!dist/**/*.js.map",
    "!dist/**/*.css.map"
  ],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": [
    "./dist/core.css"
  ],
  "scripts": {
    "prebuild": "npm run build --workspace @extable/sequence",
    "pretest": "npm run build --workspace @extable/sequence",
    "build": "vite build && tsc -p tsconfig.build.json --emitDeclarationOnly",
    "build:release": "EXTABLE_RELEASE=1 vite build && tsc -p tsconfig.build.json --emitDeclarationOnly",
    "test": "vitest run",
    "lint": "biome lint .",
    "format": "biome format --write ."
  },
  "dependencies": {
    "@extable/sequence": "0.3.9"
  },
  "devDependencies": {
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=22"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shibukawa/extable.git"
  },
  "homepage": "https://shibukawa.github.io/extable/",
  "author": "Yoshiki Shibukawa",
  "license": "Apache-2.0",
  "peerDependencies": {}
}
